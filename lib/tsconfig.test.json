{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "strict": true,
    "target": "ES2020",
    "module": "CommonJS",
    "forceConsistentCasingInFileNames": true,
    "allowUmdGlobalAccess": false,
    "moduleResolution": "node",
    "lib": [
      "ES2020",
      "dom"
    ]
  },
  "include": ["./**/*.test.ts"],
  "exclude": ["node_modules", "build", "dist"]
}
